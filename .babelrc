{
  "presets": ["next/babel"],

  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ],
    [
      "prismjs",
      {
        "languages": ["javascript", "css", "html", "jsx"],
        "plugins": ["line-numbers", "show-language"],
        "theme": "okaidia",
        "css": true
      }
    ],
    ["babel-plugin-macros"],
    ["transform-remove-console"]
  ]
}
